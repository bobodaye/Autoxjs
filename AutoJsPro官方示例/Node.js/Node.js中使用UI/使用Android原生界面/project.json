{
  "name": "Node.js使用Android原生界面",
  "main": "main.js",
  "ignore": [
    "build"
  ],
  "packageName": "com.example.ui.node.android",
  "versionName": "1.0.0",
  "versionCode": 1,
  "androidResources": {
    "resDir": "res",
    "manifest": "AndroidManifest.xml"
  },
  "permissionConfig": {
    "manifestPermissions": [
      "android.permission.INTERNET",
      "android.permission.WRITE_EXTERNAL_STORAGE",
      "android.permission.WAKE_LOCK"
    ]
  }
}